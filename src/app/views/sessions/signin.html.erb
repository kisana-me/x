<% content_for :title, "既存口座進入" %>

<h1>既存口座進入</h1>

<%= form_with(model: @account, scope: :signin, url: signin_path, method: :post, class: "ctmf", data: { turbo: false }) do |form| %>
  <%= render "shared/error_messages", instead: @account %>
  <div>
    <%= form.label :name_id %>
    <%= form.text_field :name_id %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password, value: @account.password, data: { controller: "ctmf-password" } %>
  </div>
  <div>
    <%= form.submit "進入" %>
  </div>
<% end %>
